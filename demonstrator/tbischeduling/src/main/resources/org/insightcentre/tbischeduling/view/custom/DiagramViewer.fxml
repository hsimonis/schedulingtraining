<Tab text="Process Diagram View" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1"
     fx:controller="org.insightcentre.tbischeduling.controller.custom.DiagramViewerController">
    <BorderPane fx:id="container">
        <center>
            <ScrollPane hbarPolicy="AS_NEEDED" vbarPolicy="AS_NEEDED">
            <ImageView fx:id="diagramImage" pickOnBounds="true" preserveRatio="true">
            </ImageView>
            </ScrollPane>
        </center>
        <left>
            <TreeView fx:id="treeView" prefWidth="100" >
                <TreeItem fx:id="treeRoot" value = "Processes" expanded="true">
                    <children>
                    </children>
                </TreeItem>
            </TreeView>
        </left>
        <bottom>
            <HBox alignment="center" spacing="5.0">
                <Label text="Show Duration:" />
                <ChoiceBox fx:id="showDurationBox" onAction="#onShowSelect" />
                <Label text="Show Process:" />
                <CheckBox fx:id="showProcess" onAction="#onShowSelect" />
                <Label text="Show Stage:" />
                <CheckBox fx:id="showStage" onAction="#onShowSelect" />
                <Label text="Show Machines:" />
                <CheckBox fx:id="showMachines" onAction="#onShowSelect" />
                <Label text="Show Cumulative:" />
                <CheckBox fx:id="showCumulative" onAction="#onShowSelect" />

            </HBox>
        </bottom>

    </BorderPane>
</Tab>
