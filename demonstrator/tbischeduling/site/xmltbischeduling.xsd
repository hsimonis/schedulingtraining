			<xs:attribute name="process" use="required" type="xs:IDREF"/>
			<xs:attribute name="stage" use="required" type="xs:integer"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="product">
		<xs:complexType>
			<xs:attribute name="applicationDataset" use="required" type="xs:IDREF"/>
			<xs:attribute name="id" use="required" type="xs:ID"/>
			<xs:attribute name="name" use="required" type="xs:string"/>
			<xs:attribute name="defaultProcess" use="required" type="xs:IDREF"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="resourceNeed">
		<xs:complexType>
			<xs:attribute name="applicationDataset" use="required" type="xs:IDREF"/>
			<xs:attribute name="id" use="required" type="xs:ID"/>
			<xs:attribute name="name" use="required" type="xs:string"/>
			<xs:attribute name="disjunctiveResource" use="required" type="xs:IDREF"/>
			<xs:attribute name="processStep" use="required" type="xs:IDREF"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="resourceUtilization">
		<xs:complexType>
			<xs:attribute name="applicationDataset" use="required" type="xs:IDREF"/>
			<xs:attribute name="id" use="required" type="xs:ID"/>
