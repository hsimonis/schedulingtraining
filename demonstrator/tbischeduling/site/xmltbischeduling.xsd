			<xs:attribute name="bound" use="required" type="xs:double"/>
			<xs:attribute name="flowtime" use="required" type="xs:integer"/>
			<xs:attribute name="gap" use="required" type="xs:double"/>
			<xs:attribute name="makespan" use="required" type="xs:integer"/>
			<xs:attribute name="maxEarliness" use="required" type="xs:integer"/>
			<xs:attribute name="maxLateness" use="required" type="xs:integer"/>
