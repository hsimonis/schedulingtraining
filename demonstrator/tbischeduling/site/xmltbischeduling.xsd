		</xs:complexType>
	</xs:element>
	<xs:element name="processSequence">
		<xs:complexType>
			<xs:attribute name="applicationDataset" use="required" type="xs:IDREF"/>
			<xs:attribute name="id" use="required" type="xs:ID"/>
			<xs:attribute name="name" use="required" type="xs:string"/>
			<xs:attribute name="after" use="required" type="xs:IDREF"/>
			<xs:attribute name="before" use="required" type="xs:IDREF"/>
			<xs:attribute name="offset" use="required" type="xs:integer"/>
			<xs:attribute name="sequenceType" use="required" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="processStep">
