			<xs:attribute name="solverBackend" use="required" type="xs:string"/>
			<xs:attribute name="solverStatus" use="required" type="xs:string"/>
			<xs:attribute name="time" use="required" type="xs:double"/>
			<xs:attribute name="timeout" use="required" type="xs:integer"/>
			<xs:attribute name="weightEarliness" use="required" type="xs:integer"/>
			<xs:attribute name="weightFlowtime" use="required" type="xs:integer"/>
			<xs:attribute name="weightLateness" use="required" type="xs:integer"/>
			<xs:attribute name="weightMakespan" use="required" type="xs:integer"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="task">
		<xs:complexType>
			<xs:attribute name="applicationDataset" use="required" type="xs:IDREF"/>
			<xs:attribute name="id" use="required" type="xs:ID"/>
