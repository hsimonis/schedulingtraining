		</xs:complexType>
	</xs:element>
	<xs:element name="job">
		<xs:complexType>
			<xs:attribute name="applicationDataset" use="required" type="xs:IDREF"/>
			<xs:attribute name="id" use="required" type="xs:ID"/>
			<xs:attribute name="name" use="required" type="xs:string"/>
			<xs:attribute name="order" use="required" type="xs:IDREF"/>
			<xs:attribute name="process" use="required" type="xs:IDREF"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="jobAssignment">
		<xs:complexType>
			<xs:attribute name="applicationDataset" use="required" type="xs:IDREF"/>
			<xs:attribute name="id" use="required" type="xs:ID"/>
			<xs:attribute name="name" use="required" type="xs:string"/>
			<xs:attribute name="duration" use="required" type="xs:integer"/>
			<xs:attribute name="early" use="required" type="xs:integer"/>
			<xs:attribute name="end" use="required" type="xs:integer"/>
