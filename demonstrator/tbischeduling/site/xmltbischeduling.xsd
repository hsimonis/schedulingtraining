			<xs:attribute name="ganttWidth" use="required" type="xs:integer"/>
			<xs:attribute name="horizon" use="required" type="xs:integer"/>
			<xs:attribute name="timeResolution" use="required" type="xs:integer"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="solution">
		<xs:complexType>
			<xs:attribute name="applicationDataset" use="required" type="xs:IDREF"/>
