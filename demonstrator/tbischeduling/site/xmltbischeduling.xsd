			<xs:attribute name="nrLate" use="required" type="xs:integer"/>
			<xs:attribute name="objectiveValue" use="required" type="xs:integer"/>
			<xs:attribute name="percentEarly" use="required" type="xs:double"/>
			<xs:attribute name="percentLate" use="required" type="xs:double"/>
			<xs:attribute name="solverRun" use="required" type="xs:IDREF"/>
			<xs:attribute name="solverStatus" use="required" type="xs:string"/>
			<xs:attribute name="totalEarliness" use="required" type="xs:integer"/>
			<xs:attribute name="totalLateness" use="required" type="xs:integer"/>
			<xs:attribute name="weightedEarliness" use="required" type="xs:double"/>
