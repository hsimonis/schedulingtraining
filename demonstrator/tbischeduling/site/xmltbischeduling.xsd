		</xs:complexType>
	</xs:element>
	<xs:element name="cumulativeProfile">
		<xs:complexType>
			<xs:attribute name="applicationDataset" use="required" type="xs:IDREF"/>
			<xs:attribute name="id" use="required" type="xs:ID"/>
			<xs:attribute name="name" use="required" type="xs:string"/>
			<xs:attribute name="capacity" use="required" type="xs:integer"/>
			<xs:attribute name="cumulativeResource" use="required" type="xs:IDREF"/>
			<xs:attribute name="from" use="required" type="xs:integer"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="cumulativeResource">
		<xs:complexType>
			<xs:attribute name="applicationDataset" use="required" type="xs:IDREF"/>
