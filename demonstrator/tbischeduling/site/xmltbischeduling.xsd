			<xs:attribute name="name" use="required" type="xs:string"/>
			<xs:attribute name="shortName" use="required" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="downtime">
		<xs:complexType>
			<xs:attribute name="applicationDataset" use="required" type="xs:IDREF"/>
			<xs:attribute name="id" use="required" type="xs:ID"/>
			<xs:attribute name="name" use="required" type="xs:string"/>
			<xs:attribute name="disjunctiveResource" use="required" type="xs:IDREF"/>
			<xs:attribute name="duration" use="required" type="xs:integer"/>
			<xs:attribute name="end" use="required" type="xs:integer"/>
			<xs:attribute name="endDate" use="required" type="xs:dateTime"/>
			<xs:attribute name="start" use="required" type="xs:integer"/>
			<xs:attribute name="startDate" use="required" type="xs:dateTime"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="inputError">
		<xs:complexType>
			<xs:attribute name="applicationDataset" use="required" type="xs:IDREF"/>
			<xs:attribute name="id" use="required" type="xs:ID"/>
			<xs:attribute name="name" use="required" type="xs:string"/>
			<xs:attribute name="classDesc" use="required" type="xs:string"/>
			<xs:attribute name="description" use="required" type="xs:string"/>
			<xs:attribute name="field" use="required" type="xs:string"/>
			<xs:attribute name="item" use="required" type="xs:string"/>
			<xs:attribute name="severity" use="required" type="xs:string"/>
			<xs:attribute name="value" use="required" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="intermediateSolution">
		<xs:complexType>
			<xs:attribute name="applicationDataset" use="required" type="xs:IDREF"/>
